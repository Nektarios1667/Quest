#perfmode 0
// Wait until the symbol system is ready
wait =<ready>, 1000

func playerInBounds x1 y1 x2 y2
	// Check position
	num playerX, =<playercoord_x>
	num playerY, =<playercoord_y>

	if =playerX >= =x1 && =playerX <= =x2 && =playerY >= =y1 && =playerY <= =y2
		return true
    endif

	return false
endfunc

while .main true
	playerInBounds x1:0, y1:0, x2:10, y2:10
	if =[return]
		log Player in top left corner
		breakwhile .main
    endif
endwhile .main