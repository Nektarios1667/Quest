#perfmode 0
// Wait until the symbol system is ready
wait =<ready>, 1000

func playerInBounds, x1, y1, x2, y2
	// Check position
	num pX, =<playercoord_x>
	num pY, =<playercoord_y>

	if =pX >= =x1 && =pX <= =x2 && =pY >= =y1 && =pY <= =y2
		return true
    endif

	return false
endfunc

while .main, =<currentlevel> == 'islands/islands'
	playerInBounds x1:0, y1:0, x2:10, y2:10
	if =[return]
		log Player in top left corner
		breakwhile .main
    endif
endwhile .main
